import java.util.Scanner;

public class ElementSearch {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
        int ele=sc.nextInt();
        for(int i=0;i<n;i++){
            if(arr[i]==ele){
                System.out.println(i);
                break;
            }
        }
    }
}
